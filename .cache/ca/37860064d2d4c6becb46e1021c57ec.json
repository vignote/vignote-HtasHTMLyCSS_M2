**********************************************************************************/\r\n/*                                       MAIN                                      */\r\n/***********************************************************************************/\r\nmain {\r\n  background-color: var(--bg-color);\r\n  grid-area: content;\r\n  padding: 10px 10px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/*********  MENU *********/\r\nmain nav {\r\n  opacity: 0;\r\n  pointer-events: none;\r\n  position: fixed;\r\n  top: 50px;\r\n  left: 0;\r\n}\r\n\r\nmain nav ul {\r\n  width: 150px;\r\n  padding-left: 0;\r\n  margin: 0;\r\n}\r\n\r\nmain nav ul li:hover {\r\n  background-color: var(--dark-bg-color);\r\n  font-weight: bold;\r\n}\r\n\r\n#menu:target {\r\n  opacity: 1;\r\n  pointer-events: auto;\r\n}\r\n\r\nmain nav ul li {\r\n  list-style-type: none;\r\n  min-width: 100px;\r\n  border: 1px solid black;\r\n  padding: 5px;\r\n  text-align: center;\r\n  transition: all 0.2s ease-in-out;\r\n  background-color: var(--slight-dark-bg-color);\r\n}\r\n\r\nmain nav ul li a {\r\n  text-decoration: none;\r\n  color: var(--font-color);\r\n}\r\n\r\nmain nav ul li#selected {\r\n  background-color: var(--dark-bg-color);\r\n  font-weight: bold;\r\n}\r\n\r\nmain h2 {\r\n  text-decoration: underline;\r\n}\r\n\r\n/*********  SECTION *********/\r\nmain section#intro img {\r\n  max-width: 250px;\r\n}\r\n\r\nmain section#intro h2 {\r\n  margin-top: 0;\r\n}\r\n\r\n/*********  ARTICLES *********/\r\nsection#articulos-destacados article {\r\n  text-align: center;\r\n}\r\n\r\nsection#articulos-destacados article img {\r\n  max-width: 50%;\r\n}\r\n\r\nsection#articulos-destacados article p {\r\n  margin-top: 0px;\r\n}\r\n\r\n/***********************************************************************************/\r\n/*                                      FOOTER                                     */\r\n/***********************************************************************************/\r\nfooter {\r\n  background-color: var(--dark-bg-color);\r\n  box-sizing: border-box;\r\n  padding: 10px 10px;\r\n}\r\n\r\nfooter a {\r\n  text-decoration: underline;\r\n  color: var(--font-color);\r\n}\r\n\r\nfooter a:hover {\r\n  font-weight: bold;\r\n}\r\n\r\nfooter div.redes-sociales img {\r\n  max-width: 40px;\r\n  margin: 5px 10px;\r\n}\r\n\r\n/***********************************************************************************/\r\n/***********************************************************************************/\r\n/***********************************************************************************/\r\n/*                                    MOBILE PHONE                                 */\r\n/***********************************************************************************/\r\n\r\n@media (min-width: 600px) {\r\n  body {\r\n    max-width: 1000px;\r\n  }\r\n\r\n  header h1 {\r\n    text-align: center;\r\n    display: block;\r\n  }\r\n  /*********  MENU *********/\r\n\r\n  #hamburguer-menu {\r\n    display: none;\r\n  }\r\n\r\n  main nav {\r\n    opacity: 1;\r\n    pointer-events: auto;\r\n    position: initial;\r\n  }\r\n\r\n  main nav ul {\r\n    width: unset;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    padding: 0px;\r\n    margin: 0px 0px 30px 0px;\r\n  }\r\n\r\n  main nav ul li:hover {\r\n    transform: scale(1.1);\r\n  }\r\n\r\n  /*********  INTRO *********/\r\n\r\n  main section#intro {\r\n    display: grid;\r\n    grid-template-columns: 1fr 2fr;\r\n    column-gap: 15px;\r\n  }\r\n\r\n  main section#intro img {\r\n    object-fit: cover;\r\n    width: 100%;\r\n    max-height: 100%;\r\n  }\r\n\r\n  /*********  ARTICULOS *********/\r\n  section#articulos-destacados {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-around;\r\n  }\r\n\r\n  section#articulos-destacados article img {\r\n    max-width: 200px;\r\n  }\r\n\r\n  /*********  FOOTER *********/\r\n\r\n  footer {\r\n    grid-area: footer;\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n  }\r\n\r\n  footer div.contacto {\r\n    justify-self: start;\r\n  }\r\n\r\n  footer div.redes-sociales {\r\n    justify-self: end;\r\n  }\r\n\r\n  /*\r\n  footer {\r\n    height: unset;\r\n    grid-template-columns: 1fr;\r\n  }\r\n\r\n  footer div.contacto {\r\n    justify-self: center;\r\n    text-align: center;\r\n  }\r\n\r\n  footer div.redes-sociales {\r\n    justify-self: center;\r\n  }\r\n  */\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"07efa2450a4f037b120a01ca78306267","cacheData":{"env":{}}}