or: var(--dark-bg-color);\r\n  box-sizing: border-box;\r\n  padding: 10px 10px;\r\n}\r\n\r\nfooter div.contacto {\r\n  justify-self: start;\r\n}\r\n\r\nfooter a {\r\n  text-decoration: underline;\r\n  color: var(--font-color);\r\n}\r\n\r\nfooter a:hover {\r\n  font-weight: bold;\r\n}\r\n\r\nfooter div.redes-sociales {\r\n  justify-self: end;\r\n}\r\n\r\nfooter div.redes-sociales img {\r\n  max-width: 40px;\r\n  margin: 5px 10px;\r\n}\r\n\r\n/***********************************************************************************/\r\n/***********************************************************************************/\r\n/***********************************************************************************/\r\n/*                                    MOBILE PHONE                                 */\r\n/***********************************************************************************/\r\n\r\n@media screen and (max-width: 600px) {\r\n  body {\r\n    max-width: none;\r\n    height: unset;\r\n  }\r\n\r\n  header {\r\n    height: unset;\r\n  }\r\n\r\n  header img#menu-icon {\r\n    display: inline;\r\n  }\r\n\r\n  main {\r\n    height: unset;\r\n  }\r\n\r\n  /*********  MENU *********/\r\n  main nav {\r\n    opacity: 0;\r\n    pointer-events: none;\r\n    position: fixed;\r\n    top: 50px;\r\n    left: 0;\r\n  }\r\n\r\n  main nav ul {\r\n    display: block;\r\n    width: 150px;\r\n  }\r\n\r\n  main nav ul li:hover {\r\n    background-color: var(--dark-bg-color);\r\n    transform: none;\r\n    font-weight: bold;\r\n  }\r\n\r\n  #menu:target {\r\n    opacity: 1;\r\n    pointer-events: auto;\r\n  }\r\n\r\n  /*********  INTRO *********/\r\n\r\n  main section#intro {\r\n    grid-template-columns: 1fr;\r\n    justify-items: center;\r\n  }\r\n  main section#intro img {\r\n    max-width: 250px;\r\n  }\r\n\r\n  /*********  ARTICLES *********/\r\n  section#articulos-destacados {\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n\r\n  /*********  FOOTER *********/\r\n  foo